<sequence xmlns="http://ws.apache.org/ns/synapse" name="apns_in">
   <in>
      <apns.dispatchToDevice configKey="apns_config" xmlns:m0="http://services.samples">
         <deviceToken>{$body/m0:pushMessage/m0:message/m0:deviceToken}</deviceToken>
         <alert>{$body/m0:pushMessage/m0:message/m0:alert}</alert>
         <sound>{$body/m0:pushMessage/m0:message/m0:sound}</sound>
         <badge>{$body/m0:pushMessage/m0:message/m0:badge}</badge>
      </apns.dispatchToDevice>
      <header name="To" action="remove"/>
      <property name="NO_ENTITY_BODY" scope="axis2" action="remove"/>
      <property name="RESPONSE" value="true"/>
      <send/>
      <log level="full"/>
   </in>
</sequence>
